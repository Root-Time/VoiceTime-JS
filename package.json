        "prisma": "^4.9.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@discordjs/rest": "^1.3.0",
        "@prisma/client": "4.9.0",
        "@types/sqlite3": "^3.1.8",
        "date-fns": "^2.29.3",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.3",
        "sqlite3": "^5.1.2"
